{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.cs"],
          "exclude": [ "**/bin/**", "**/obj/**" ], 
          "src": "./Bandwidth.Net.Extra"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
    ],
    "overwrite": [
    ],
    "dest": ".doc",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "Bandwidth.Net.Extra helpers",
      "_appFooter": "Copyright Â© Bandwidth"
    },
    "template": [
      "statictoc"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
